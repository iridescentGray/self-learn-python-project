   "source": [
    "# 自动求导机制\n",
    "会自动计算返向传播全部"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "import torch"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 手动指定\n",
    "指定requires_grad=True 即可"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "tensor([[-1.0057, -0.2083, -0.2826],\n",
       "        [-0.7396,  1.0556, -0.2632]], requires_grad=True)"
      ]
     },
     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# requires_grad作为方法参数\n",
    "with_grad=torch.randn(2,3,requires_grad=True)\n",
    "with_grad"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "tensor([[-0.3133, -0.7584, -1.4677],\n",
       "        [ 0.1517,  1.9479,  0.0673]], requires_grad=True)"
      ]
     },
     "execution_count": 6,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# 后续指定requires_grad\n",
    "grad=torch.randn(2,3)\n",
    "grad.requires_grad=True\n",
    "grad"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## grad 链式传播\n"
   ]
  }
 ],
