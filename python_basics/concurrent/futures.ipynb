   "source": [
    "from concurrent.futures import Future\n",
    "\n",
    "\n",
    "future = Future()\n",
    "\n",
    "\n",
    "def callback(future):\n",
    "    print(\"当set_result的时候,执行回调，我也可以拿到返回值：\", future.result())\n",
    "\n",
    "\n",
    "future.add_done_callback(callback)\n",
    "# 当future执行set_result时,会触发回调函数的执行\n",
    "future.set_result(\"return value\")"
   ]
  }
 ],
